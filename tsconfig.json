{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/_layout.jsx",
    "app/index.jsx",
    "constants/AppConfig.js",
    "constants/Colors.js",
    "constants/Fonts.js",
    "constants/Routes.js",
    "hooks/useColorScheme.js",
    "hooks/useColorScheme.web.js",
    "hooks/useThemeColor.js",
    "nativewind-env.d.ts"
  ]
}